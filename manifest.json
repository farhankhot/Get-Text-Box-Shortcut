{
  "name": "Text Box select shortcut",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
		{
		"matches":["<all_urls>"], 
    "js": ["script.js"]
  	}
	],
	"background": {
        "service_worker": "background.js"
    },
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {},
  "commands": {
    "_execute_action": {
      "suggested_key": "Ctrl+Shift+Space"	
    }
  }
}